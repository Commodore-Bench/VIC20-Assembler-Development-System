.PAGE 'PRG2BYTE'.OPT NOLIST;**********************************=$1201        ;START OF BASIC;*********************************; SET BSTUB TO ONE TO ALLOW FOR; INSERTION OF BASIC STUB CODE;*********************************BSTUB = 1;*********************************;;         PRG2BYTE64;      PRG2BYTE.C64 V072882;     CONVERT PROGRAM FILE;  TO ASSEMBLER64 SOURCE FILE;          BYTE,WORD ;; (C) 1982 BY COMMODORE MACHINES;;  WRITTEN BY DENTON MARLOWE;*********************************;; BASIC FORMAT PROGRAM;; 10 SYS(4623);;*********************************.IFN BSTUB <;.IF  BSTUB == 1 .WORD BASIC    ;LINE LINK POINTER .BYTE $0A,$00  ;LINE NUMBER (10) .BYTE 158      ;SYS COMMAND TOKEN .BYTE '(4623)' ;CALL TO $120F .BYTE $00      ;END OF LINEBASIC .BYTE $00,$00  ;END OF BASIC>;.FI;*********************************; VIC20 SPECIFIC EQUATES;*********************************BASICW = $C474 ;BASIC WARM STARTCHROUE = $E742 ;OUTPUT CHAR TO SCREENSCROLY = $900F ;SCREEN CONTROL;*********************************;*=$120F        ;PROGRAM20 ORGIN;*********************************; JUMP OVER ; PROCESSOR SPECIFIC UTILITIES;*********************************       JMP START;*********************************; SET FLAG TO ALLOW BASIC; AND KERNAL (ERROR) MESSAGES; $80 FOR BASIC; $40 FOR KERNAL;*********************************;;*********************************SETMSG=$FF90;*********************************START  LDA #$40     ;ALLOW KERNAL MESSAGES       JSR SETMSG   ;KERNAL FLAG;*********************************; MAIN BODY OF CODE;*********************************.FIL PRG2BYTE.ASM;*********************************.END